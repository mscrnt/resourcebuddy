# ResourceSpace API Configuration
# Copy this file to .env and fill in your actual values

# ResourceSpace API URL (without trailing slash)
VITE_RS_API_URL=https://your-resourcespace-domain.com/api

# ResourceSpace Admin API Key
VITE_RS_API_KEY=your_api_key_here

# ResourceSpace Admin User
VITE_RS_USER=admin

# Demo Mode (set to true to use mock data)
VITE_DEMO_MODE=false

# Backend URL (for settings and auth)
VITE_BACKEND_URL=http://localhost:3003

# Cache API URL (for Redis and cache management)
VITE_CACHE_URL=http://localhost:8000